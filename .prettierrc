{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "useTabs": false,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "css",
  "proseWrap": "always"
}
